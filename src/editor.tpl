{{ target: wysw-main }}
<link rel="stylesheet" type="text/css" href="${baseUrl}/editor.css">
<div class="editor-toolbar">
    <p class="toolbar-title">工具栏</p>
    <span class="glyphicon glyphicon-chevron-left toolbar-icon" aria-hidden="true"></span>
    <div class="editor-operate">
        <button id="btn-ok" class="btn btn-success btn-self">确定</button>
        <button id="btn-reset" class="btn btn-warning btn-self">重置</button>
        <button id="btn-cancel" class="btn btn-danger btn-self">取消</button>
    </div>
    <div class="editor-icon"></div>
</div>
<div id="editor-droppable" class="col-md-12 col-sm-12 col-xs-12 editor-droppable"></div>
<div class="side-bar-bg">
</div>
